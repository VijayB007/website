<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Product Listing</title>
</head>
<body>
    <h1>Products</h1>
    <div id="product-list">
        {{#each products}}
        <div class="product" >
            <h2>{{name}}</h2>
            <p>{{description}}</p>
            <p>Price: ${{price}}</p>
            <img src="{{image_url}}" alt="{{name}}" style="max-width: 200px;">
        </div>
        {{/each}}
       
    </div>
</body>
</html>
